{
  "name": "agentic-platform",
  "version": "1.0.0",
  "description": "Production-grade agentic platform with multi-LLM support, project scaffolding, and containerized development environments",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server/index.ts",
    "build": "tsc && esbuild src/server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "node dist/server/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t agentic-platform:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/"
  },
  "keywords": [
    "agentic",
    "llm",
    "platform",
    "multi-provider",
    "ai",
    "sandbox",
    "development"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "socket.io": "^4.7.2",
    "dockerode": "^4.0.2",
    "dotenv": "^17.2.2",
    "uuid": "^9.0.1",
    "fs-extra": "^11.2.0",
    "chokidar": "^3.5.3",
    "ngrok": "^5.0.5",
    "js-tiktoken": "^1.0.12",
    "axios": "^1.6.5",
    "winston": "^3.11.0",
    "pino": "^8.17.2",
    "redis": "^4.6.12",
    "postgres": "^3.4.3",
    "drizzle-orm": "^0.44.5",
    "stripe": "^14.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.9.3",
    "tsx": "^4.19.1",
    "esbuild": "^0.25.0",
    "vitest": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "prettier": "^3.6.2",
    "drizzle-kit": "^0.31.4"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
